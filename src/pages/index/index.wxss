page {
  background: #fff;
}

.banner image {
  width: 100%;
  height: 364rpx;
  background: #eee;
}

.commodity {
  padding: 32rpx;
}

.vip {
  padding: 32rpx;
}

.title {
  position: relative;
  padding-left: 20rpx;
  font-size: 16px;
  color: #363636;
}

.title::after {
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -15rpx;
  content: '';
  width: 6rpx;
  height: 30rpx;
  background: #fb5f0a;
}

.commodity .content {
  margin-top: 24rpx;
}

.commodity .content .img {
  width: 332rpx;
  height: 344rpx;
  background: #eee;
}

.commodity .content .name {
  margin-top: 20rpx;
}

.vip .icon {
  width: 88rpx;
  height: 88rpx;
}

.vip .item {
  padding: 24rpx 0;
  padding-left: 36rpx;
  border-bottom: 1rpx solid #f6f6f6;
}

.vip .name {
  font-size: 14px;
}

.mid {
  flex: 1;
  margin-left: 44rpx;
  font-size: 14px;
}

.prize {
  height: 106rpx;
}

.prize .icon {
  font-size: 12px;
}

.name-1004 {
  color: #F26132;
}
.name-1003 {
  color: #118BD9;
}
.name-1002 {
  color: #B7905A;
}
.name-1001 {
  color: #06865D;
}

.text-overflow {
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}