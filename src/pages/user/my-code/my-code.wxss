page{
  background: #2D3131;
  width:100%;
  height:100%;
}
.cont-view{
  display: flex;
  width:100%;
  justify-content: center;
}
.cont-view>view{
  margin-top: 160rpx;
  width:670rpx;
  height:882rpx;
  background: #FFFFFF;
  border-radius: 2px;

}
.codes{
  text-align: center;
}
.head-img image{
  width:126rpx;
  height:126rpx;
}
.codes>image{
